<?xml version="1.0" encoding="utf-8" ?>
<job maxFilesPerProcess="1" fileListSyntax="paths">
<!-- <job maxFilesPerProcess="1" filesPerHour="1" fileListSyntax="xrootd"> -->
	<command>
         cd ${SUBMITTINGDIRECTORY}
  setenv STARFPE NO
  set evtsFile = $INPUTFILE0;
  echo "${evtsFile}"
  if (! -r ${evtsFile}) exit 1;
  set file     = `basename $evtsFile .root`
  set dir      = `dirname  $evtsFile`;          
  set logFile  = ${file}B.log
  if ( -r $logFile) exit 0;
  root.exe -q -b 'TpcAligner.C(9999999,"'${evtsFile}'")' &gt;&amp;  ${logFile}
	</command>
	<stdout URL="file:/star/u/fisyak/prodlog/$JOBID.log" />
	<stderr URL="file:/star/u/fisyak/prodlog/$JOBID.err" />
