<job maxFilesPerProcess="1" filesPerHour="1" fileListSyntax="paths">
	<command>
         cd ${SUBMITTINGDIRECTORY}
  setenv STARFPE NO
  setenv NODEBUG yes
  setup 64b
  foreach  evtsFile(`cat $FILELIST`) 
#  set evtsFile = $INPUTFILE0;
  echo "${evtsFile}"
  if (! -r ${evtsFile}) continue;
  set file     = `basename $evtsFile .root`
  set dir      = `dirname  $evtsFile`;          
  set logFile  = ${file}B.log
  if ( -r $logFile) continue;
  root.exe -q -b 'TpcAligner.C(9999999,"'${evtsFile}'")' &gt;&amp;  ${logFile}
  end
	</command>
	<stdout URL="file:./sched$JOBID.log" />
	<stderr URL="file:./sched$JOBID.err" />
<input URL="command:dEdxSample.pl" />
