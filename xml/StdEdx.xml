<job name="dEdx" maxFilesPerProcess="1" filesPerHour="1" simulateSubmission="false" fileListSyntax="paths">
<command>
         cd ${SUBMITTINGDIRECTORY}
         set file = `basename ${INPUTFILE0} .event.root`;
         set log  = ${file}.log         
         set rootf  = ${file}.root
         if (-r ${rootf}) exit 0;
         root.exe -q -b  'dEdx.C(50000,"'${INPUTFILE0}'","'${rootf}'",2)'  &gt;&amp; ${log}
</command>
<stdout URL="file:/star/u/fisyak/prodlog/dEdx_sched$JOBID.log" />
<stderr URL="file:/star/u/fisyak/prodlog/dEdx_sched$JOBID.err" />

